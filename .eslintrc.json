{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["@typescript-eslint"],
  "rules": {},
  "overrides": [
    {
      "files": ["jest.setup.js", "jest.config.js"],
      "rules": {
        "no-restricted-syntax": "off",
        "no-restricted-imports": "off"
      }
    }
  ]
}
